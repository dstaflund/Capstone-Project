    <string name="intro_label">Sask. Geo-Memorial</string>
    <string name="intro_project_title">The Project</string>
    <string name="intro_project_description">Saskatchewan honours residents who have worked or died in service of the Province by naming geographic landmarks after them</string>
    <string name="intro_app_title">The App</string>
    <string name="intro_app_description">The Geo-Memorial app gives detailed information on the locations of these landmarks and on the residents they were named after</string>
    <string name="intro_map_title">The Map</string>
    <string name="intro_map_description">See where the geo-memorials are by interacting with the map</string>
    <string name="intro_search_title">Searching</string>
    <string name="intro_search_description">Search geo-memorials by name, location, or resident</string>
    <string name="intro_favorites_title">Favorites</string>
    <string name="intro_favorites_description">Keep a list of your favorite geo-memorials for later viewing</string>
    <string name="intro_share_title">Sharing</string>
    <string name="intro_share_description">Share geo-memorials with colleagues, friends, and family</string>
    <string name="intro_settings_title">Settings</string>
